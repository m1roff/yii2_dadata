{
  "name": "m1roff/yii2-dadata",
  "description": "Handy module for using dadata.ru service",
  "type": "yii2-extension",
  "keywords": ["yii2","extension","dadata", "suggestion"],
  "license": "MIT",
  "minimum-stability": "stable",
  "support": {
    "source": "https://github.com/m1roff/yii2-dadata"
  },
  "authors": [
    {
      "name": "Miroff",
      "email": "m@miroff.su"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0",
    "yiisoft/yii2-bootstrap": "^2.0",
    "yiisoft/yii2-jui": "^2.0",
    "skeeks/yii2-dadata-suggest-api": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "m1roff\\dadata\\": "src/"
    }
  },
  "archive": {
    "exclude": [
      ".gitignore",
      "composer.lock"
    ]
  },
  "extra": {
    "bootstrap": "m1roff\\dadata\\Bootstrap"
  }
}
