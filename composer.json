{
  "name": "mirkhamidov/yii2-dadata",
  "description": "Handy module for using dadata.ru service",
  "type": "yii2-extension",
  "keywords": ["yii2","extension","dadata", "suggestion"],
  "license": "MIT",
  "minimum-stability": "stable",
  "support": {
    "source": "https://github.com/mirkhamidov/yii2-dadata"
  },
  "authors": [
    {
      "name": "Jasur Mirkhamidov",
      "email": "mirkhamidov.jasur@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0",
    "yiisoft/yii2-bootstrap": "^2.0",
    "yiisoft/yii2-jui": "^2.0",
    "skeeks/yii2-dadata-suggest-api": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "mirkhamidov\\dadata\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "mirkhamidov\\dadata\\Bootstrap"
  }
